{% extends 'layouts/homelayout.html.twig' %}

{% block content %}
    <div class="content">
        {% for i in pimcore_block('contentblock').iterator %}
            <h2>{{ pimcore_input('subline') }}</h2>
            {{ pimcore_wysiwyg('content') }}
        {% endfor %}
        <div class="e-date">
            {{ pimcore_date('myDate', {
                'format': 'd.m.Y',
                'outputFormat': '%d.%m.%Y'
            })
            }}
        </div>
        {{ pimcore_areablock("customAreablock") }}
        {{ pimcore_numeric('myNumber') }}
    </div>
    {{ pimcore_snippet('footerSnippet') }}
{% endblock %}